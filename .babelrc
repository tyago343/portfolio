{
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }
      ]
    ],
    "plugins": ["@babel/plugin-proposal-class-properties"],
    "sourceMaps": true,
    "retainLines": true
  }